{
  "redirects": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "docs.brokle.com" }],
      "destination": "https://brokle.com/docs",
      "permanent": false
    },
    {
      "source": "/:path+",
      "has": [{ "type": "host", "value": "docs.brokle.com" }],
      "destination": "https://brokle.com/docs/:path+",
      "permanent": false
    }
  ]
}
